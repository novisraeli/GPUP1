<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>

<BorderPane minHeight="0.0" minWidth="0.0" prefHeight="750.0" prefWidth="1300.0" styleClass="background" stylesheets="@../utility/backgroung.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="FXML.graph.graphController">
   <bottom>
      <HBox fx:id="hbox" alignment="TOP_RIGHT" BorderPane.alignment="TOP_RIGHT">
         <BorderPane.margin>
            <Insets bottom="5.0" right="30.0" />
         </BorderPane.margin>
         <children>
            <Label text="Name:">
               <HBox.margin>
                  <Insets right="10.0" top="5.0" />
               </HBox.margin>
            </Label>
            <TextField fx:id="nameOfFile">
               <HBox.margin>
                  <Insets right="10.0" />
               </HBox.margin>
            </TextField>
            <Button id="fileButton" fx:id="loadFileButton" minWidth="119.0" mnemonicParsing="false" onAction="#loadFileButton" prefHeight="27.0" prefWidth="119.0" styleClass="fileButton" text="Path" />
         </children>
      </HBox>
   </bottom>
   <center>
      <Pane fx:id="paneImage" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
   </center>
</BorderPane>
